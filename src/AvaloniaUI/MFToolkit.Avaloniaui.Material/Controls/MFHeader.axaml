<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MFToolkit.Avaloniaui.Material.Controls.MFHeader"
             x:Name="This"
             Margin="5 10">
    <Grid ColumnDefinitions="Auto,*,Auto">
        <ContentPresenter Grid.Column="0" Content="{Binding #This.LeftContent}"></ContentPresenter>
        <ContentPresenter Grid.Column="1" Content="{Binding #This.CenterContent}"></ContentPresenter>
        <ContentPresenter Grid.Column="2" Content="{Binding #This.RightContent}"></ContentPresenter>
    </Grid>
</UserControl>