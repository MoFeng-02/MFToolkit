<Grid xmlns="https://github.com/avaloniaui"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
      x:Class="MFToolkit.Avaloniaui.Material.Controls.MFCellItem"
      ColumnDefinitions="*,Auto"
      Name="CellItem">
    <ContentPresenter Content="{Binding #CellItem.LeftContent}"
                      HorizontalAlignment="Stretch" Padding="{Binding #CellItem.LeftContentPadding}"
                      Background="Transparent" />
    <ContentPresenter Content="{Binding #CellItem.RightContent}" Grid.Column="1"
                      HorizontalAlignment="Stretch" Padding="{Binding #CellItem.RightContentPadding}"
                      Background="Transparent" />
</Grid>