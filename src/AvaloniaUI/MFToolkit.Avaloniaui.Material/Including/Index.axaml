<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Add Styles Here -->
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceInclude Source="/ColorTheme/LightTheme.axaml" x:Key="Light" />
                <ResourceInclude Source="/ColorTheme/DarkTheme.axaml" x:Key="Dark" />
            </ResourceDictionary.ThemeDictionaries>
            <CornerRadius x:Key="SmallCornerRadius">7</CornerRadius>
            <CornerRadius x:Key="MediumCornerRadius">15</CornerRadius>
        </ResourceDictionary>
    </Styles.Resources>
    <StyleInclude Source="/Styles/ButtonStyles.axaml" />
    <StyleInclude Source="/Styles/ComboBoxStyles.axaml" />
    <StyleInclude Source="/Styles/ComboBoxItemStyles.axaml" />
    <StyleInclude Source="/Styles/ListBoxStyles.axaml" />
    <StyleInclude Source="/Styles/ListBoxItemStyles.axaml" />
    <StyleInclude Source="/Styles/MFButtonStyles.axaml" />
    <StyleInclude Source="/Styles/MFSpinStyles.axaml" />
    <StyleInclude Source="/Styles/MFCellStyles.axaml" />
    <StyleInclude Source="/Styles/MFCellItemStyles.axaml" />
    <StyleInclude Source="/Styles/MFSwitchStyles.axaml" />
    <StyleInclude Source="/Styles/MFProgressBarStyles.axaml" />
</Styles>